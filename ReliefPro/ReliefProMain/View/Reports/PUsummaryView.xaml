                        <DataGridTextColumn Header="Device #" Binding="{Binding psv.PSVName}"/>
                        <DataGridTextColumn Header="Protected System"  Binding="{Binding psv.PSVName}"/>
                        <DataGridTextColumn Header="Device Type"  Binding="{Binding psv.ValveType}"/>
                        <DataGridTextColumn Header="Set Pressure,MPag" Binding="{Binding psv.Pressure}"/>
                        <DataGridTextColumn Header="Discharge To" Binding="{Binding psv.DischargeTo}"/>

