                                    <StackPanel HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
                                        <Border>
                                            <Label Content="Controlling Single Scenario" Width="340" HorizontalContentAlignment="Center"/>
                                        </Border>
                                        <Border>
                                         <GridSplitter BorderBrush="Black" BorderThickness="0.5"   HorizontalAlignment="Stretch"/>
                                        </Border>
                                        <Border>
                                            <StackPanel Width="340" Orientation="Horizontal">
                                                <StackPanel Width="75">
                                                    <Label Content="Relief Rate,"/>
                                                    <Label Content="Kg/hr"/>
                                                </StackPanel>
                                                <GridSplitter BorderBrush="Black" BorderThickness="0.5"/>
                                                <Label Content="Phase" Width="50" 
                                                HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
                                                <GridSplitter BorderBrush="Black" BorderThickness="0.5"/>
                                                <StackPanel Width="70">
                                                    <Label Content="MW"/>
                                                    <Label Content="or SpGr"/>
                                                </StackPanel>
                                                <GridSplitter BorderBrush="Black" BorderThickness="0.5"/>
                                                <Label Content="T,C" Width="70" 
                                                HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
                                                <GridSplitter BorderBrush="Black" BorderThickness="0.5"/>
                                                <Label Content="Z" Width="70" 
                                                HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
                                            </StackPanel>
                                        </Border>
                                    </StackPanel>
                                </DataTemplate>
                            </DataGridTemplateColumn.HeaderTemplate>
