                                            <TextBlock Margin="1,0,1,0"  Width="77"
                                        Text="{Binding SingleDS.ReliefLoad}" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                        </Border>
                                        <Border>
                                            <GridSplitter Background="Black" BorderThickness="0.5" BorderBrush="Black" Margin="0,-1,0,-1"/>
                                        </Border>
                                        <Border>
                                            <TextBlock Margin="1,0,1,0"  Width="48"
