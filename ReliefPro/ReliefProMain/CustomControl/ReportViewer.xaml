<UserControl x:Class="ReliefProMain.CustomControl.ReportViewer"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:wf="clr-namespace:Microsoft.Reporting.WinForms;assembly=Microsoft.ReportViewer.WinForms"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <WindowsFormsHost Name="winFormsHost" VerticalAlignment="Stretch" HorizontalAlignment="Stretch">
        <wf:ReportViewer x:Name="rptViewer"/>
    </WindowsFormsHost>
</UserControl>
